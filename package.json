{
  "name": "raphaelle-grassigli",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "source-map-explorer": "^2.1.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "// START DEV SERVER": "",
    "start": "react-scripts start",
    "// ANALYZE BUILD SIZE": "",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "// DEPLOYMENT": "",
    "build": "react-scripts build",
    "copy-to-s3": "aws s3 cp build s3://923847569348756348975/ --recursive",
    "invalidate-cache": "aws cloudfront create-invalidation --distribution-id E1EOJ4CADOIKXD --paths /index.html /favicon.ico",
    "deploy": "npm run build && npm run copy-to-s3 && npm run invalidate-cache"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "aws-cli": "^0.0.2",
    "typescript-plugin-css-modules": "^2.0.2"
  }
}
