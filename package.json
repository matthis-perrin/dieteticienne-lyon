{
  "name": "raphaelle-grassigli",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^5.1.2",
    "node-sass": "^4.13.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-ga": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "source-map-explorer": "^2.1.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "// START DEV SERVER": "",
    "start": "react-scripts start",
    "// ANALYZE BUILD SIZE": "",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "// DEPLOYMENT": "",
    "build": "react-scripts build",
    "copy-to-s3": "aws s3 cp build s3://dieteticienne-lyon-website/ --recursive",
    "invalidate-cache": "aws cloudfront create-invalidation --distribution-id E3196T4DW19S80 --paths /index.html /favicon.ico",
    "deploy": "npm run build && npm run copy-to-s3 && npm run invalidate-cache"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "typescript-plugin-css-modules": "^2.0.2"
  }
}
